<script>
        $(document).ready(function() {
            // Select elements by their IDs and store them in variables
            var getOrderElement = $('#getOrder');
  var contactTele = $('#contactTele');
            var increaseTrendElement = $('#increaseTrend');
            var decreaseTrendElement = $('#decreaseTrend');
            var ignoreTrendElement = $('#ignoreTrend');
            var loadingScreenElement = $('#loadingScreen');

            function start() {

                
                
                getOrderElement.hide();
                loadingScreenElement.show()
                $.get("/api/getCurrent", function(response) {
                    if (response.success) {
                        var trend = response.result.toLowerCase();
                        if (trend === "up") {
                            setTimeout(() => {
                                loadingScreenElement.hide()
                                increaseTrendElement.show();
                                decreaseTrendElement.hide();
                                ignoreTrendElement.hide();
                            }, 5000)
                            // Show only increaseTrend element, hide others
                            
                        } else if (trend === "down") {
                            // Show only decreaseTrend element, hide others
                            setTimeout(() => {
                                loadingScreenElement.hide()
                                increaseTrendElement.hide();
                                decreaseTrendElement.show();
                                ignoreTrendElement.hide();
                            }, 5000)

                        } else if (trend === "sideway") {
                            // Show only ignoreTrend element, hide others
                            
                            setTimeout(() => {
                                loadingScreenElement.hide()
                                increaseTrendElement.hide();
                                decreaseTrendElement.hide();
                                ignoreTrendElement.show();
                            }, 5000)
                        } else {
                            console.log("Unknown trend:", trend);
                        }

                        // Display time difference if available
                        if (response.timeDiff) {
                            var timeRemaining = response.timeDiff; // Time difference in seconds

                            // Update the UI with initial time remaining
                            $(".timeDiff").text("Số liệu sẽ được làm mới sau: " + timeRemaining + "s");

                            // Create a countdown timer
                            var countdown = setInterval(function() {
                                timeRemaining--;
                                $(".timeDiff").text("Số liệu sẽ được làm mới sau: " + timeRemaining + "s");

                                if (timeRemaining <= 0) {
                                    clearInterval(countdown); // Stop the countdown
                                    // After countdown finishes, call start() again after 2000ms
                                    setTimeout(function() {
                                        start();
                                    }, 2000);
                                }
                            }, 1000); // Update timer every second (1000ms)
                        }

                        $(".randPercent").html(Math.floor(Math.random() * (80 - 60 + 1)) + 60)

                    } else {
                        // Alert message from JSON if success is not true
                        alert(response.result);
                        contactTele.show();
                        loadingScreenElement.hide()
                    }
                }).fail(function() {
                    console.error("Failed to fetch data from /api/getCurrent");
                    // Optionally, handle failure cases here
                });
                
            }

        
            $("#startBtn").on("click", start)


        
            
        });
        </script>
         
        
